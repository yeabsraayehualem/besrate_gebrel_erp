<?xml version="1.0" encoding="utf-8"?>
<odoo>

<record id="class_attendance_view_list" model="ir.ui.view">
    <field name="name">class.attendance.view.list</field>
    <field name="model">class.attendance</field>
    <field name="arch" type="xml">
        <list string="Class Attendance" create="0" >
            <field name="student_id"/>
            <field name="attendance_state"/>
            <field name="attendance_date"/>
            <field name="stage_id"/>
        </list>
    </field>
</record>

<record id="class_attendance_action" model="ir.actions.act_window">
    <field name="name">Class Attendance</field>
    <field name="res_model">class.attendance</field>
    <field name="view_mode">list</field>
    <field name="domain">[]</field>
    <field name="context">{}</field>
    <field name="help" type="html">
        <p>No attendance data</p>
    </field>
</record>

<record id="feast_attendance_view_list" model="ir.ui.view">
    <field name="name">feast.attendance.view.list</field>
    <field name="model">feast.attendance</field>
    <field name="arch" type="xml">
        <list string="Feast Attendance" create="0" >
            <field name="student_id"/>
            <field name="attendance_state"/>
            <field name="attendance_date"/>
            <field name="stage_id"/>
        </list>
    </field>
</record>

<record id="feast_attendance_action" model="ir.actions.act_window">
    <field name="name">Holy Feast Attendance</field>
    <field name="res_model">feast.attendance</field>
    <field name="view_mode">list</field>
    <field name="domain">[]</field>
    <field name="context">{}</field>
    <field name="help" type="html">
        <p>No attendance data</p>
    </field>
</record>

</odoo>
